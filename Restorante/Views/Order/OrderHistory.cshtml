@model List<Restorante.Models.OrderDetailsViewModels.OrderDetailViewModel>
@{
    ViewData["Title"] = "OrderHistory";
}

<h2>Order History</h2>

<br />

@if (Model.Count > 0)
{
    @foreach (var item in Model)
    {
        @Html.Partial("_IndividualOrderDetails", item);
    }

}
else
{
    <p>No Past Orders Available</p>
}
<br />